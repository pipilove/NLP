  <action name="actionOpenFileEvent">
   <property name="text">
    <string>openFileEvent</string>
   </property>
   <property name="toolTip">
    <string>def openFileEvent(self):
    # if self.maybeSave():        # 如果先前被打开的文件已被修改，需要提示
    fileName, _ = QtWidgets.QFileDialog.getOpenFileName(self)
    file = QtCore.QFile(fileName)
    if not file.open(QtCore.QFile.ReadOnly | QtCore.QFile.Text):
        QtWidgets.QMessageBox.warning(self, &quot;analysis&quot;,
                                      &quot;file %s can't be readed:\n%s.&quot; % (fileName, file.errorString()))
        return

    inf = QtCore.QTextStream(file)
    QtWidgets.QApplication.setOverrideCursor(QtCore.Qt.WaitCursor)
    self.text_input.setPlainText(inf.readAll())
    QtWidgets.QApplication.restoreOverrideCursor()

    # self.setCurrentFile(fileName)
    self.statusBar().showMessage(&quot;file readed successfully&quot;, 2000)</string>
   </property>
  </action>
